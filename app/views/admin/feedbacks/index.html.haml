- if !flash[:notice].blank?
  .alert.alert-info
    = flash[:notice]

%br/
%br/
%table.table.table-bordered.table-striped
  %thead
    %tr
      %th Name
      %th Download Link
      %th Meeting Time
      %th
  %tbody
    - @feedbacks.each do |feedback|
      %tr
        %td= feedback.name
        %td= link_to "Download Resume", feedback.attachment_url
        %td= feedback.date.strftime("%H:%M %m/%d/%Y")
        %td= button_to "Delete", admin_feedback_path(feedback), method: :delete, class: "btn btn-danger", data: { confirm: "Are you sure that you wish to delete #{feedback.name}?" }